extends layout

block content
  .container
    h2 Resultado de la ronda
    h3 #{g.player1} = #{g.currentRound.score[0]}
    h3 #{g.player2} = #{g.currentRound.score[1]}
    br
    h2 Resultado Parcial del Juego
    h3 #{g.player1} = #{g.score[0]}
    h3 #{g.player2} = #{g.score[1]}
    br
    form(role='form', action="/resultadoround",method="post", style='max-width: 300px;')
      .form-group
        input.form-control(type="hidden", name='gameid', value='#{g._id}')
        button.button1(type='submit') Continuar
      &nbsp;
